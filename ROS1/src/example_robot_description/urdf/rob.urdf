      <origin xyz="0 0 0" />
      <mass value="0" />
      <inertia ixx="0.0" ixy="0.0" ixz="0.0"
               iyy="0.0" iyz="0.0"
               izz="0.0" />
    </inertial>
  </link>

  <joint name="imu_join" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
  </joint>



  <!--

    !!!!!
    GAZEBO SECTION
    !!!!!

  -->


  <!-- JOINT STATES -->
  <gazebo>

    <plugin name="gazebo_ros_joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
      <robotNamespace>robot</robotNamespace>
      <topicName>joint_states</topicName>
      <!--<argument>joint_states:=joint_states</argument>-->
      <joint>wheel_lf_joint</joint>
      <joint>wheel_lb_joint</joint>
      <joint>wheel_rf_joint</joint>
      <joint>wheel_rb_joint</joint>
    </plugin>

    <plugin name="field_robot_diff_drive_controller" filename="libgazebo_ros_skid_steer_drive.so">
      <updateRate>50</updateRate>

      <robotNamespace>robot</robotNamespace>

      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>wheel_encoders/odom</odometryTopic>

      <publish_odom>true</publish_odom>
      <broadcastTF>false</broadcastTF>

      <robotBaseFrame>base_footprint</robotBaseFrame>
      <odometryFrame>odom</odometryFrame>


      <leftFrontJoint>wheel_lf_joint</leftFrontJoint>
      <rightFrontJoint>wheel_rf_joint</rightFrontJoint>

      <leftRearJoint>wheel_lb_joint</leftRearJoint>
      <rightRearJoint>wheel_rb_joint</rightRearJoint>


      <wheelSeparation>0.4875</wheelSeparation>
      <!-- <wheel_separation>0.475</wheel_separation>
      <wheel_separation>0.5</wheel_separation> -->

      <wheelDiameter>0.24669</wheelDiameter>

      <torque>10</torque>

    </plugin>

  </gazebo>

  <!-- CAMERA PLUGINS/SENSORS -->
  <gazebo reference="camera_front_virtual_link">
    <sensor type="camera" name="camera_front">
      <visualize>true</visualize>
      <updateRate>30</updateRate>
      <camera name="camera_front">
        <horizontal_fov>2.1</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.007</stddev>
        </noise>
        <distortion>
          <k1>0.0</k1>
          <k2>0.0</k2>
          <k3>0.0</k3>
          <p1>0.0</p1>
          <p2>0.0</p2>
          <center>0.5 0.5</center>
        </distortion>
      </camera>
      <plugin name="gazebo_cam_front_plugin" filename="libgazebo_ros_camera.so">

        <robotNamespace>robot</robotNamespace>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>

        <cameraName>camera_front</cameraName>
        <frameName>camera_front_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

