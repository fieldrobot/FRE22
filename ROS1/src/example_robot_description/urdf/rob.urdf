  <!-- BASE FOOTPRINT -->
  <link name="base_footprint"/>

  <!-- BASE LINK -->
  <link name="base_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.4 0.21 0.05"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.4 0.21 0.05"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="6.6666667e-03" ixy="0" ixz="0"
               iyy="6.6666667e-03" iyz="0"
               izz="6.6666667e-03" />
    </inertial>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.2" rpy="0 0 0"/>
  </joint>

  <gazebo reference="base_link">
    <material>Gazebo/DarkGrey</material>
  </gazebo>


  <!-- WHEEL LEFT Front -->
  <link name="wheel_lf">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.123345"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.123345"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="3" />
      <inertia ixx="4.0675e-02" ixy="0" ixz="0"
               iyy="4.0675e-02" iyz="0"
               izz="8e-02" />
    </inertial>
  </link>

  <joint name="wheel_lf_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_lf"/>
    <origin xyz="0.1685 0.155 -0.035" rpy="-1.57079632679 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <gazebo reference="wheel_lf">
    <!--<mu1>20.0</mu1>
    <mu2>20.0</mu2>-->
    <!--<kp>100000000.0</kp>
    <kd>10000.0</kd>-->
    <!--<minDepth>0.001</minDepth>-->
    <material>Gazebo/FlatBlack</material>
    <selfCollide>false</selfCollide>
    <mu1 value="0.5"/>
    <mu2 value="0.5"/>
    <kp value="10000000.0" />
    <kd value="1" />
    <fdir1 value="1 0 0"/>
  </gazebo>

<!-- WHEEL LEFT BACK -->
  <link name="wheel_lb">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.06" radius="0.123345"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
